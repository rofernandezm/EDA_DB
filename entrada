createTable(Personas)
createTable(CI)
addCol(Personas, CI, integer, PRIMARY_KEY)
addCol(Personas, Apellido, string, NOT_EMPTY)
addCol(Personas, Nombre, string, ANY)
insertInto(Personas, CI:Apellido:Nombre, 1111111:Perez:Ana)
insertInto(Personas, CI:Apellido:Nombre, 1111111:Perez:Fernando)
insertInto(Personas, CI:Apellido:Nombre, 2222222:Perez:Fernando)
insertInto(Personas, CI:Apellido:Nombre, 3333333:Gonzalez:Maria)
alterCol(Personas, Nombre, string, NOT_EMPTY, Nombre)
printdatatable(Personas)
printMetadata(Personas)
printTables()
salir()
